<function name="zsp_dce_geom"
		window-func="false"
		returns-setof="false"
		behavior-type="CALLED ON NULL INPUT"
		function-type="VOLATILE"
		security-type="SECURITY INVOKER"
		parallel-type="PARALLEL UNSAFE"
		execution-cost="100"
		row-amount="0">
	<schema name="public"/>
	<role name="postgres"/>
	<language name="plpgsql"/>
	<return-type>
	<type name="text" length="0"/>
	</return-type>
	<parameter name="_param1">
		<type name="text" length="0"/>
	</parameter>
	<definition> <![CDATA[declare
	type_geometry ALIAS FOR $1;
	msg text;
begin
if (type_geometry = 'p') then
	begin
	begin
	  drop table dce_point_geom;
	  exception
	  when UNDEFINED_TABLE THEN
	end;
	execute 'create table dce_point_geom as select trait_id,
	  trait.pos_deb_long_dd,
	  trait.pos_deb_lat_dd
	  from trait
	  where (trait.pos_deb_long_dd is not null and trait.pos_deb_lat_dd is not null and trait.pos_fin_long_dd is null and trait.pos_fin_lat_dd is null)
	  order by trait_id';
	execute 'select * from AddGeometryColumn(''dce_point_geom'',''trait_geom'',4326,''POINT'',2)';
	execute 'update dce_point_geom set trait_geom=st_geomfromtext(''POINT(''||pos_deb_long_dd||'' ''||pos_deb_lat_dd||'')'',4326)';
	execute 'alter table dce_point_geom add constraint point_pkey PRIMARY KEY(trait_id)';
  
	return 'points ok';
	end;

elsif (type_geometry = 'l')  then
	begin
	begin
	  drop table dce_ligne_geom;
	  exception
	  when UNDEFINED_TABLE THEN
	end;
	execute 'create table dce_ligne_geom as select trait_id,
	  trait.pos_deb_long_dd,
	  trait.pos_deb_lat_dd,
	  trait.pos_fin_long_dd,
	  trait.pos_fin_lat_dd
	  from trait
	  where (trait.pos_deb_long_dd is not null and trait.pos_deb_lat_dd is not null and trait.pos_fin_long_dd is not null and trait.pos_fin_lat_dd is not null)
	  order by trait_id';
	execute 'select * from AddGeometryColumn(''dce_ligne_geom'',''trait_geom'',4326,''LINESTRING'',2)';
	execute 'update dce_ligne_geom set trait_geom=st_geomfromtext(''LINESTRING(''||pos_deb_long_dd||'' ''||pos_deb_lat_dd||'',''||pos_fin_long_dd||'' ''||pos_fin_lat_dd||'')'',4326)';
	execute 'alter table dce_ligne_geom add constraint ligne_pkey PRIMARY KEY(trait_id)';
	return 'lignes ok';
	end;

else
	msg = 'type valide p ou l';
	return msg;
	exit;
end if;

end;]]> </definition>
</function>

