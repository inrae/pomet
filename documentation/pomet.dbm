<view name="v_campagne" layers="0" collapse-mode="2" max-obj-count="0" z-value="0">
	<schema name="public"/>
	<role name="&quot;eric.quinton&quot;"/>
	<tag name="campagne"/>
	<position x="1760" y="700"/>
	<simplecol name="campagne_id" type="integer"/>
	<simplecol name="campagne_nom" type="character varying(100)"/>
	<simplecol name="fk_masse_eau" type="integer"/>
	<simplecol name="saison" type="character varying(10)"/>
	<simplecol name="annee" type="integer"/>
	<simplecol name="fk_personne" type="integer"/>
	<simplecol name="is_actif" type="smallint"/>
	<simplecol name="experimentation_id" type="integer"/>
	<simplecol name="masse_eau" type="character varying(255)"/>
	<simplecol name="code_agence" type="character varying(255)"/>
	<definition> <![CDATA[SELECT c.campagne_id,
    c.campagne_nom,
    c.fk_masse_eau,
    c.saison,
    c.annee,
    c.fk_personne,
    c.is_actif,
    c.experimentation_id,
    masse_eau.masse_eau,
    masse_eau.code_agence
   FROM (campagnes c
